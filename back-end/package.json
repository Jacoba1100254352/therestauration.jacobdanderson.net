{
  "name": "back-end",
	"version": "2.0.0",
  "description": "",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
	"test": "echo \"Error: no test specified\" && exit 1",
	"start": "node dist/server.js",
	"build": "tsc",
	"watch": "tsc -w",
	"dev": "nodemon --exec ts-node src/server.ts",
	"lint": "eslint src/**/*.ts",
	"lint-fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
	"@types/cookie-parser": "^1.4.7",
	"@types/cookie-session": "^2.0.48",
	"@types/readline-sync": "^1.4.8",
	"argon2": "^0.40.1",
	"body-parser": "^1.20.2",
	"cookie-parser": "^1.4.6",
	"cookie-session": "^2.1.0",
	"express": "^4.19.2",
	"express-session": "^1.18.0",
	"mongoose": "^8.2.4",
	"readline-sync": "^1.4.10"
  },
  "devDependencies": {
	"@types/body-parser": "^1.19.5",
	"@types/express": "^4.17.21",
	"@types/express-session": "^1.18.0",
	"@types/node": "^20.12.2",
	"@typescript-eslint/eslint-plugin": "^7.4.0",
	"@typescript-eslint/parser": "^7.4.0",
	"eslint": "^8.57.0",
	"nodemon": "^3.1.0",
	"ts-node": "^10.9.2",
	"typescript": "^5.4.3"
  },
  "eslintConfig": {
	"env": {
	  "es2021": true,
	  "node": true
	},
	"extends": [
	  "eslint:recommended",
	  "plugin:@typescript-eslint/recommended"
	],
	"overrides": [
	  {
		"env": {
		  "node": true
		},
		"files": [
		  ".eslintrc.{js,cjs}"
		],
		"parserOptions": {
		  "sourceType": "script"
		}
	  }
	],
	"parserOptions": {
	  "ecmaVersion": "latest",
	  "parser": "@typescript-eslint/parser",
	  "sourceType": "module"
	},
	"plugins": [
	  "@typescript-eslint"
	],
	"rules": {
	  "indent": [
		"error",
		"tab"
	  ],
	  "linebreak-style": [
		"error",
		"unix"
	  ],
	  "quotes": [
		"error",
		"double"
	  ],
	  "semi": [
		"error",
		"always"
	  ]
	}
  }
}
